{
  "version": 2,
  "builds": [
    {
      "src": "home/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "home/static/$1"
    }
  ]
}